<XmlMessage xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>TreasureHuntRequestMessage</Name>
	<Id>6488</Id>
	<Heritage>Message</Heritage>
	<Fields>
		<XmlField>
			<Name>questLevel</Name>
			<Type>byte</Type>
			<Condition>questLevel &lt; 1 || questLevel &gt; 200</Condition>
		</XmlField>
		<XmlField>
			<Name>questType</Name>
			<Type>sbyte</Type>
			<Condition>questType &lt; 0</Condition>
		</XmlField>
	</Fields>
</XmlMessage>