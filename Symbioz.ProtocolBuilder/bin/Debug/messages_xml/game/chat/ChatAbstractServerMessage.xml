<XmlMessage xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>ChatAbstractServerMessage</Name>
	<Id>880</Id>
	<Heritage>Message</Heritage>
	<Fields>
		<XmlField>
			<Name>channel</Name>
			<Type>sbyte</Type>
			<Condition>channel &lt; 0</Condition>
		</XmlField>
		<XmlField>
			<Name>content</Name>
			<Type>utf</Type>
		</XmlField>
		<XmlField>
			<Name>timestamp</Name>
			<Type>int</Type>
			<Condition>timestamp &lt; 0</Condition>
		</XmlField>
		<XmlField>
			<Name>fingerprint</Name>
			<Type>utf</Type>
		</XmlField>
	</Fields>
</XmlMessage>