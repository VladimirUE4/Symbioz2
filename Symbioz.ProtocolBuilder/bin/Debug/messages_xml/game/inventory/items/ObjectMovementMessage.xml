<XmlMessage xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>ObjectMovementMessage</Name>
	<Id>3010</Id>
	<Heritage>Message</Heritage>
	<Fields>
		<XmlField>
			<Name>objectUID</Name>
			<Type>varuhint</Type>
			<Condition>objectUID &lt; 0</Condition>
		</XmlField>
		<XmlField>
			<Name>position</Name>
			<Type>byte</Type>
			<Condition>position &lt; 0 || position &gt; 255</Condition>
		</XmlField>
	</Fields>
</XmlMessage>