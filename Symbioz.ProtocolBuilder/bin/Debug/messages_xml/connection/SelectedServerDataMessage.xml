<XmlMessage xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>SelectedServerDataMessage</Name>
	<Id>42</Id>
	<Heritage>Message</Heritage>
	<Fields>
		<XmlField>
			<Name>serverId</Name>
			<Type>varuhshort</Type>
			<Condition>serverId &lt; 0</Condition>
		</XmlField>
		<XmlField>
			<Name>address</Name>
			<Type>utf</Type>
		</XmlField>
		<XmlField>
			<Name>port</Name>
			<Type>ushort</Type>
			<Condition>port &lt; 0 || port &gt; 65535</Condition>
		</XmlField>
		<XmlField>
			<Name>canCreateNewCharacter</Name>
			<Type>boolean</Type>
		</XmlField>
		<XmlField>
			<Name>ticket</Name>
			<Type>sbyte[]</Type>
		</XmlField>
	</Fields>
</XmlMessage>